39\n\n\u041e\u043f\u0442\u043e\u0432\u0430\u044f \u043f\u043e\u0441\u0442\u0430\u0432\u043a\u0430 \u043a\u0438\u0441\u043b\u043e\u0442\u044b \u0438 \u0449\u0435\u043b\u043e\u0447\u0438 \u0432 \u043f\u043b\u0430\u0441\u0442\u0438\u043a\u043e\u0432\u044b\u0445 \u043a\u0430\u043d\u0438\u0441\u0442\u0440\u0430\u0445 \u043f\u043e 20-30 \u043b\u0438\u0442\u0440\u043e\u0432\n\n\u041e\u043f\u0442\u043e\u0432\u044b\u0435 \u043f\u043e\u0441\u0442\u0430\u0432\u043a\u0438 \u043a\u0438\u0441\u043b\u043e\u0442 \u0438 \u0449\u0435\u043b\u043e\u0447\u0435\u0439  \u0432  \u043f\u043b\u0430\u0441\u0442\u0438\u043a\u043e\u0432\u044b\u0445 \u043a\u0443\u0431\u043e\u0432\u044b\u0445 \u0435\u043c\u043a\u043e\u0441\u0442\u044f\u0445 \u043f\u043e 1000\n\u043b\u0438\u0442\u0440\u043e\u0432\n\n\u041f\u043e\u0441\u0442\u0430\u0432\u043a\u0430 \u0430\u0437\u043e\u0442\u043d\u043e\u0439 \u043a\u0438\u0441\u043b\u043e\u0442\u044b \u0432 \u0441\u0442\u0435\u043a\u043b\u044f\u043d\u043d\u044b\u0445 \u0431\u0443\u0442\u044b\u043b\u044f\u0445 \u043f\u043e 20 \u043b\u0438\u0442\u0440\u043e\u0432\n\n\u0425\u0440\u0430\u043d\u0438\u043b\u0438\u0449\u0435 \u0445\u0438\u043c\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u0438\n\n\u041a\u043e\u043b\u043e\u043d\u043d\u0430 \u0434\u043b\u044f \u043d\u0430\u043b\u0438\u0432\u0430 \u0432 \u0430\u0432\u0442\u043e\u0446\u0438\u0441\u0442\u0435\u0440\u043d\u0443 \u043a\u0438\u0441\u043b\u043e\u0442\u044b \u0438 \u0449\u0435\u043b\u043e\u0447\u0438\n\n\u0413\u043e\u0442\u043e\u0432\u0430\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044f \u0433\u0438\u043f\u043e\u0445\u043b\u043e\u0440\u0438\u0442\u0430 \u043d\u0430\u0442\u0440\u0438\u044f\n\n\u0421\u043e\u0434\u0430 \u043a\u0430\u0443\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0447\u0435\u0448\u0443\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f \u0432 \u043c\u0435\u0448\u043a\u0435 \u043f\u043e 25 \u043a\u0433\n\n\u0421\u043e\u0434\u0430 \u043a\u0430\u0443\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0433\u0440\u0430\u043d\u0443\u043b\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f \u0432 \u043c\u0435\u0448\u043a\u0430\u0445 \u043f\u043e 25 \u043a\u0433\n\n\u0421\u043e\u0434\u0430 \u043a\u0430\u043b\u044c\u0446\u0438\u043d\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0430\u044f \u0432 \u043c\u0435\u0448\u043a\u0430\u0445 \u043f\u043e 25 \u043a\u0433\n\n\u041f\u0435\u0440\u0435\u0432\u043e\u0437\u043a\u0430 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u043c\u0438 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0430\u0432\u0442\u043e\u0446\u0438\u0441\u0442\u0435\u0440\u043d\u0430\u043c\u0438 \u043f\u043e \u0432\u0441\u0435\u0439 \u0420\u043e\u0441\u0441\u0438\u0438\n\u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0439 \u0445\u0438\u043c\u0438\u0438.\n\n\u041f\u043e\u0441\u0442\u0430\u0432\u043a\u0430 \u043a\u0438\u0441\u043b\u043e\u0442 \u0430\u0437\u043e\u0442\u043d\u043e\u0439, \u0441\u0435\u0440\u043d\u043e\u0439,\u0441\u043e\u043b\u044f\u043d\u043e\u0439 \u0441\u043b\u0438\u0432 \u0432 \u0442\u0430\u0440\u0443 \u041f\u043e\u0442\u0440\u0435\u0431\u0438\u0442\u0435\u043b\u044f\n\n\u041f\u043e\u0441\u0442\u0430\u0432\u043a\u0430 \u0449\u0435\u043b\u043e\u0447\u0438 (\u043d\u0430\u0442\u0440 \u0435\u0434\u043a\u0438\u0439 \u0436\u0438\u0434\u043a\u0438\u0439 ) \u043d\u0430 \u0441\u043b\u0438\u0432 \u0432 \u0442\u0430\u0440\u0443 \u041f\u043e\u0442\u0440\u0435\u0431\u0438\u0442\u0435\u043b\u044f\n\n# \u041e \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438\n\n\u041f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u0430\u044f \u0445\u0438\u043c\u0438\u044f \u043e\u043f\u0442\u043e\u043c \u0441 \u0434\u043e\u0441\u0442\u0430\u0432\u043a\u043e\u0439 \u043f\u043e \u0420\u043e\u0441\u0441\u0438\u0438 \u0438 \u0421\u041d\u0413\n\n\u041f\u0440\u0438\u043e\u0440\u0438\u0442\u0435\u0442\u043d\u044b\u043c \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u041e\u041e\u041e \u00ab\u0426\u0435\u043d\u0442\u0440\u0421\u043d\u0430\u0431\u00bb \u0441\u0442\u0430\u043b\u0430 \u043f\u0440\u043e\u0434\u0430\u0436\u0430 \u0432\u044b\u0441\u043e\u043a\u043e\u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439\n\u043f\u0440\u043e\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0439 \u0445\u0438\u043c\u0438\u0438 \u0438\u0437 \u041a\u0438\u0442\u0430\u044f.\n\n\u041d\u0430\u0434\u0435\u0436\u043d\u044b\u0435 \u0438 \u043d\u0430\u043b\u0430\u0436\u0435\u043d\u043d\u044b\u0435 \u0441\u0432\u044f\u0437\u0438 \u0441 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f\u043c\u0438 \u041a\u0438\u0442\u0430\u0439\u0441\u043a\u043e\u0439 \u041d\u0430\u0440\u043e\u0434\u043d\u043e\u0439\n\u0414\u0435\u043c\u043e\u043a\u0440\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u0420\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0438 \u043f\u043e\u0437\u0432\u043e\u043b\u044f\u044e\u0442 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438 \u043f\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0442\u044c \u0441\u0432\u043e\u0438\u043c \u043f\u043e\u0442\u0440\u0435\u0431\u0438\u0442\u0435\u043b\u044f\u043c\n\u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0435 \u0445\u0438\u043c\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0441\u044b\u0440\u044c\u0435, \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044e\u0449\u0435\u0435 \u0432\u0441\u0435\u043c \u043c\u0435\u0436\u0434\u0443\u043d\u0430\u0440\u043e\u0434\u043d\u044b\u043c \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u0430\u043c \u0438\n\u0413\u041e\u0421\u0422\u0430\u043c, \u043f\u043e \u043c\u0438\u043d\u0438\u043c\u0430\u043b\u044c\u043d\u043e\u0439 \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438 \u0438\u0437 \u043f\u0435\u0440\u0432\u044b\u0445 \u0440\u0443\u043a \u0431\u0435\u0437 \u043f\u043e\u0441\u0440\u0435\u0434\u043d\u0438\u043a\u043e\u0432.\n\n\u0410 \u0442\u0430\u043a\u0436\u0435 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044f \u043e\u0441\u0443\u0449\u0435\u0441\u0442\u0432\u043b\u044f\u0435\u0442 \u043f\u043e\u0441\u0442\u0430\u0432\u043a\u0438 \u0445\u0438\u043c\u0438\u0447\u0435\u0441\u043a\u043e\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u0438 \u0441 \u0420\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u0438\u0445\n\u043f\u0440\u0435\u0434\u043f\u0440\u0438\u044f\u0442\u0438\u0439 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439 \u043f\u0440\u0438 \u044d\u0442\u043e\u043c \u0446\u0435\u043d\u0430 \u043e\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043d\u0438\u0436\u0435 \u0441\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u0438 \u0441\u0430\u043c\u043e\u0433\u043e\n\u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f \u0437\u0430 \u0441\u0447\u0435\u0442 \u0441\u043a\u0438\u0434\u043a\u0438 \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u043c\u043e\u0439 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u043c \u043d\u0430\u0448\u0435\u0439 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0438.\n\n\u041e\u043f\u0442\u043e\u0432\u044b\u043c \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u044f\u043c \u0438 \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u044b\u043c \u043a\u043b\u0438\u0435\u043d\u0442\u0430\u043c \u00ab\u0426\u0435\u043d\u0442\u0440\u0421\u043d\u0430\u0431\u00bb \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0435\u0442 \u0441\u0430\u043c\u044b\u0435\n\u0432\u044b\u0433\u043e\u0434\u043d\u044b\u0435 \u0443\u0441\u043b\u043e\u0432\u0438\u044f \u0440\u0430\u0431\u043e\u0442\u044b:\n\n  * \u0433\u0438\u0431\u043a\u0443\u044e \u0441\u0438\u0441\u0442\u0435\u043c\u0443 \u043e\u043f\u043b\u0430\u0442\u044b \u0441 \u043e\u0442\u0441\u0440\u043e\u0447\u043a\u043e\u0439 \u043f\u043b\u0430\u0442\u0435\u0436\u0430\n  * \u043f\u043e\u0441\u0442\u0430\u0432\u043a\u0443 \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u0438 \u043f\u043e\u0434 \u0437\u0430\u043a\u0430\u0437\n  * \u0440\u0435\u0437\u0435\u0440\u0432\u0438\u0440\u043e\u0432\u0430\u043d\u0438\u0435 \u043f\u0430\u0440\u0442\u0438\u0439 \u0442\u043e\u0432\u0430\u0440\u0430\n  * \u044d\u043a\u0441\u043a\u043b\u044e\u0437\u0438\u0432\u043d\u044b\u0435 \u0443\u0441\u043b\u043e\u0432\u0438\u044f \u0438 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0435 \u0446\u0435\u043d\u044b \u0434\u043b\u044f \u043a\u0440\u0443\u043f\u043d\u044b\u0445 \u043e\u043f\u0442\u043e\u0432\u044b\u0445 \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u0435\u0439\n\n\u041d\u0430 \u0442\u0435\u043a\u0443\u0449\u0438\u0439 \u043c\u043e\u043c\u0435\u043d\u0442 \u0438\u043c\u0435\u0435\u0442\u0441\u044f \u043f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e\u0435 \u043d\u0430\u043b\u0438\u0447\u0438\u0435 \u043d\u0430 \u0441\u043a\u043b\u0430\u0434\u0435 \u0432 \u041c\u043e\u0441\u043a\u043e\u0432\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438\n(\u0433. \u041b\u043e\u0431\u043d\u044f) \u0442\u0430\u043a\u0438\u0435 \u043f\u043e\u0437\u0438\u0446\u0438\u0438, \u043a\u0430\u043a \u0442\u0440\u0438\u0431\u0443\u0442\u0438\u043b\u0444\u043e\u0441\u0444\u0430\u0442, \u0442\u0440\u0438\u0438\u0437\u043e\u0431\u0443\u0442\u0438\u043b\u0444\u043e\u0441\u0444\u0430\u0442 \u0438\n\u0442\u0440\u0438\u0445\u043b\u043e\u0440\u044d\u0442\u0438\u043b\u0435\u043d. \u0410\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0435 \u0446\u0435\u043d\u044b \u043d\u0430 \u044d\u0442\u0438 \u0442\u043e\u0432\u0430\u0440\u044b \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0443\u0437\u043d\u0430\u0442\u044c \u043f\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0443 \u0438\u043b\u0438\n\u043e\u0441\u0442\u0430\u0432\u0438\u0432 \u0437\u0430\u044f\u0432\u043a\u0443 \u043d\u0430 \u0441\u0430\u0439\u0442\u0435.\n\n## \u041f\u043e\u0447\u0435\u043c\u0443 \u0432\u044b\u0431\u0438\u0440\u0430\u044e\u0442 \u043d\u0430\u0441\n\n  * 1\u00ab\u0426\u0435\u043d\u0442\u0440\u0421\u043d\u0430\u0431\u00bb- \u0441\u0442\u0430\u043b \u0434\u0438\u043b\u0435\u0440\u043e\u043c \u043c\u043d\u043e\u0433\u0438\u0445 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439 \u0445\u0438\u043c\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0432\u0435\u0449\u0435\u0441\u0442\u0432 \u043a\u0430\u043a \u0420\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u0438\u0445 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439 \u0442\u0430\u043a \u0438 \u043a\u0440\u0443\u043f\u043d\u044b\u0445 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u044b\u0445 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439 \u041a\u0438\u0442\u0430\u044f \u0438 \u0415\u0432\u0440\u043e\u043f\u044b, \u0438 \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u043c\u043d\u043e\u0433\u043e\u043b\u0435\u0442\u043d\u0438\u043c \u0441\u0432\u044f\u0437\u044f\u043c \u0441 \u0437\u0430\u0432\u043e\u0434\u0430\u043c\u0438, \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0435\u0442 \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044e \u043a\u0430\u043a \u0441\u043e \u0441\u0432\u043e\u0438\u0445 \u0441\u043a\u043b\u0430\u0434\u0441\u043a\u0438\u0445 \u043f\u043b\u043e\u0449\u0430\u0434\u0435\u0439, \u0442\u0430\u043a \u0438 \u043d\u0430\u043f\u0440\u044f\u043c\u0443\u044e \u0441 \u0437\u0430\u0432\u043e\u0434\u043e\u0432 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439.\n  * 2\u041f\u043e\u0441\u0442\u043e\u044f\u043d\u043d\u043e\u0435 \u0440\u0430\u0441\u0448\u0438\u0440\u0435\u043d\u0438\u0435 \u043f\u0430\u0440\u0442\u043d\u0435\u0440\u0441\u043a\u0438\u0445 \u0441\u0432\u044f\u0437\u0435\u0439 \u0441 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f\u043c\u0438 \u043f\u043e\u0437\u0432\u043e